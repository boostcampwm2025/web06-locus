{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "tsBuildInfoFile": "../../.cache/tsbuildinfo/tsconfig.typecheck.tsbuildinfo",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "emitDeclarationOnly": true,
    "outDir": "./dist-types",
    "moduleResolution": "bundler",
    "resolvePackageJsonExports": true
  },
  "include": ["src"],
  "exclude": ["src/**/*.stories.tsx", "src/**/*.stories.ts"]
}
